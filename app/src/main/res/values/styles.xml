<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CircularSeekBarStyle">
        <item name="trackColor">?attr/colorSecondary</item>
        <item name="trackColorActive">?attr/colorPrimary</item>
        <item name="thumbColor">?attr/colorPrimaryVariant</item>

    </style>

    <style name="ShapeAppearance.Material3.SmallComponent.Circular" parent="ShapeAppearance.Material3.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="AppDialogTheme" parent="Theme.Material3.Light.Dialog.MinWidth">
        <item name="android:background">@color/purple_700</item>
        <item name="cornerFamily">rounded</item>
        <item name="cornerRadius">8dp</item>
    </style>
</resources>