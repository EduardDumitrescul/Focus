<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CircularSeekBarStyle">
        <item name="trackColor">?attr/colorSecondaryVariant</item>
        <item name="trackColorActive">?attr/colorPrimary</item>
        <item name="thumbColor">?attr/colorPrimaryVariant</item>
        <item name="trackThickness">24dp</item>
        <item name="thumbThickness">20dp</item>
        <item name="trackThicknessActive">15dp</item>

    </style>

    <style name="ShapeAppearance.Material3.SmallComponent.Circular" parent="ShapeAppearance.Material3.SmallComponent">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="App.Button" parent="ThemeOverlay.Material3.Button">
        <item name="backgroundTint">?attr/colorPrimaryDark</item>
    </style>

    <style name="App.Dialog" parent="ThemeOverlay.Material3.MaterialAlertDialog.Centered">
        <item name="colorPrimary">?attr/colorPrimaryDark</item>
        <!-- For drawable -->
        <item name="colorControlNormal">?attr/colorPrimaryDark</item>
    </style>

    <style name="App.Switch" parent="Widget.Material3.CompoundButton.Switch">
       <item name="android:textColor">?android:attr/textColorTertiary</item>
        <item name="android:textAppearance">@style/TextAppearance.App.BodyLarge</item>

    </style>

    <style name="App.TextView" parent="">
        <item name="android:textColor">?android:attr/textColorTertiary</item>
    </style>

    <style name="App.TextView.BodyLarge" parent="App.TextView">
        <item name="android:textAppearance">@style/TextAppearance.App.BodyLarge</item>
        <item name="android:paddingStart">4dp</item>
        <item name="android:paddingTop">8dp</item>
        <item name="android:paddingBottom">12dp</item>
    </style>
    <style name="App.TextView.TitleLarge" parent="App.TextView">
        <item name="android:textAppearance">@style/TextAppearance.App.TitleLarge</item>
        <item name="android:paddingStart">4dp</item>
        <item name="android:paddingTop">12dp</item>
        <item name="android:paddingBottom">16dp</item>
    </style>

    <style name="ThemeOverlay.App.Toolbar" parent="">
        <item name="colorPrimary">@color/nature_background</item>
        <item name="colorPrimaryVariant">@color/nature_primaryVariant</item>-->
        <item name="colorOnPrimary">@color/nature_primaryDark</item>
    </style>

    <style name="App.Toolbar" parent="Widget.MaterialComponents.Toolbar.Primary">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.Toolbar</item>
        <item name="titleTextAppearance">@style/TextAppearance.App.HeadlineMedium</item>
        <item name="subtitleTextAppearance">@style/TextAppearance.App.HeadlineMedium</item>
        <item name="android:textViewStyle">@style/App.TextView</item>
    </style>


</resources>