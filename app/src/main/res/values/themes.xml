<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.FocusTycoon" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_300</item>
        <item name="colorPrimaryVariant">@color/purple_500</item>
        <item name="colorPrimaryDark">@color/purple_600</item>

        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/cherry_200</item>
        <item name="colorSecondaryVariant">@color/cherry_500</item>
        <item name="colorOnSecondary">@color/black</item>


        <item name="colorError">@color/cherry_700</item>
        <item name="colorErrorContainer">@color/cherry_300</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:colorBackground">@color/purple_50</item>
        <!-- Customize your theme here. -->
    </style>
</resources>